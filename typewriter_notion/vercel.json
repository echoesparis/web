{
  "version": 2,
  "builds": [
    {
      "src": "api/notion.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/notion/blocks/([^/]+)/children",
      "dest": "/api/notion.js?blockId=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/api/notion/blocks/:blockId/children",
      "destination": "/api/notion.js?blockId=:blockId"
    }
  ]
}

